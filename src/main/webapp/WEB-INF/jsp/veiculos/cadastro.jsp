<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ page language="java" contentType="text/html; charset=ISO-8859-1" pageEncoding="ISO-8859-1" %>

<div class="mb-3 mt-3">
    <label for="marca" id="marca" class="form-label">Marca:</label>
    <input type="text" class="form-control" placeholder="Digite a marca do veículo" name="marca" required>
</div>

<div class="mb-3 mt-3">
    <label for="modelo" id="modelo" class="form-label">Modelo:</label>
    <input type="text" class="form-control" placeholder="Digite o modelo do veículo" name="modelo" required>
</div>

<div class="mb-3 mt-3">
    <label for="ano" id="ano" class="form-label">Ano:</label>
    <input type="text" class="form-control" placeholder="Digite o ano do veículo" name="ano" required>
</div>


<div class="mb-3 mt-3">
    <label for="valor" id="valor" class="form-label">Valor:</label>
    <input type="text" class="form-control" placeholder="Digite o valor do veículo" name="valor">
</div>
